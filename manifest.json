{
  "name": "Wage Rate",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Replaces dollars with hours.",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["wage-rate.js"],
      "run_at" : "document_end"
    }
  ],
  "content_security_policy": "default-src 'none'; script-src 'self'"
}
